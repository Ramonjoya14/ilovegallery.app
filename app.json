{
  "expo": {
    "name": "iLoveGallery",
    "slug": "ilovegallery",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ilra",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#120D0A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ilovegallery.app.v2",
      "buildNumber": "8",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.ilovegallery.app",
      "versionCode": 12,
      "icon": "./assets/images/android-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon.png",
        "backgroundColor": "#120D0A"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Permite que iLoveGallery guarde las fotos tomadas en tu galería.",
          "savePhotosPermission": "Permite que iLoveGallery guarde las fotos tomadas en tu galería."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Permite que iLoveGallery acceda a tus fotos para elegir una portada.",
          "cameraPermission": "Permite que iLoveGallery use tu cámara para tomar fotos del evento."
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bf3a5edd-8a3d-44fa-98e9-9ff74c01febe"
      }
    },
    "owner": "ramonjoya14",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bf3a5edd-8a3d-44fa-98e9-9ff74c01febe"
    }
  }
}