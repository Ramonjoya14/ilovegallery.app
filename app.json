{
  "expo": {
    "name": "iLoveGallery",
    "slug": "ilovegallery",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ilra",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#120D0A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ilovegallery.app.v2",
      "buildNumber": "11",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "iLoveGallery needs access to your camera so you can capture photos during events and contribute to the shared gallery.",
        "NSPhotoLibraryUsageDescription": "iLoveGallery needs access to your photos so you can select a cover image for your events.",
        "NSPhotoLibraryAddUsageDescription": "iLoveGallery needs permission to save the shared event photos to your device's gallery.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.402998744302-1bpgcptffmaums3bcht6fo0c6dd1ki60"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.ilovegallery.app",
      "versionCode": 15,
      "icon": "./assets/images/android-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon.png",
        "backgroundColor": "#120D0A"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "iLoveGallery needs to access your photos to save the event recap into your personal gallery.",
          "savePhotosPermission": "iLoveGallery needs to save photos to your library so you can keep memories from the events you attend."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "iLoveGallery needs likely access to your photos to let you pick a cover image for your new event.",
          "cameraPermission": "iLoveGallery needs access to your camera to let you take photos and participate in the event."
        }
      ],
      "@react-native-community/datetimepicker",
      "expo-apple-authentication",
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bf3a5edd-8a3d-44fa-98e9-9ff74c01febe"
      }
    },
    "owner": "ramonjoya14",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bf3a5edd-8a3d-44fa-98e9-9ff74c01febe"
    }
  }
}